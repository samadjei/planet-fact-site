f;\n\t--grey: #838391;\n\t--celestial-blue: #419ebb;\n\t--yellow: #eda249;\n\t--purple: #6d2ed5;\n\t--orange: #d14c32;\n\t--red: #d83a34;\n\t--dark-orange: #cd5120;\n\t--topaz: #1ec1a2;\n\t--blue: #2d68f0;\n\t--iron: #d8d8d8;\n\n\t// Fonts\n\t--primary-font: 'Spartan', sans-serif;\n\t--secondary-font: 'Antonio', sans-serif;\n\t--font-weight-normal: 400;\n\t--font-weight-bold: 700;\n}\n","scss/components/_buttons.scss":"@use '../abstracts' as *;\n\n// Base Button Styling\n.btn {\n\tdisplay: inline-block;\n\tfont-family: var(--primary-font);\n\tfont-weight: 700;\n\tfont-size: 1.2rem;\n\tletter-spacing: 2.57px;\n\tline-height: 2.5rem;\n\tborder-radius: 2.8rem;\n\tborder: none;\n\tcursor: pointer;\n\tcolor: var(--white);\n\ttext-transform: uppercase;\n}\n\n// BUTTON STYLES\n\n// First Button\n.btn--primary {\n\tbackground: transparent;\n\tcolor: var(--white);\n\ttransition: all 0.4s ease-in;\n\t&:hover,\n\t&:focus {\n\t\tbackground: var(--iron);\n\t}\n}\n\n// // Second Button\n// .btn--secondary {\n// \tbackground: $mirage-blue;\n// \tcolor: $link-water-white;\n// \tborder: 1px solid $link-water-white;\n// \t&:hover,\n// \t&:focus {\n// \t\tbackground: $link-water-white;\n// \t\tcolor: $mirage-blue;\n// \t}\n// }\n\n// .btn--tertiary {\n// \tbackground: transparent;\n// \tcolor: $mirage-blue;\n// \tborder: 1px solid $mirage-blue;\n// \t&:hover,\n// \t&:focus {\n// \t\tbackground: $mirage-blue;\n// \t\tcolor: $link-water-white;\n// \t}\n// \t@include breakpoint(medium) {\n// \t\twidth: 16.8rem;\n// \t\theight: 4.8rem;\n// \t}\n// \t@include breakpoint(tablet) {\n// \t\tmargin-left: 3rem;\n// \t}\n// }\n\n// // Button Sizes\n\n// // Desktop Button\n// .btn--desktop {\n// \twidth: 17.1rem;\n// \theight: 4.8rem;\n// \tmargin-left: auto;\n// \t@include breakpoint(phone) {\n// \t\twidth: 16.8rem;\n// \t\tmargin: 0 auto;\n// \t}\n// }\n\n// .btn--nav {\n// \twidth: 17.1rem;\n// \theight: 4.8rem;\n// \t@include breakpoint(tablet) {\n// \t\twidth: 16.8rem;\n// \t}\n// }\n\n// .hero__cta {\n// \tdisplay: inline-block;\n// \tfont-family: 'Public Sans';\n// \tfont-weight: 700;\n// \tfont-size: 1.5rem;\n// \tletter-spacing: -0.12px;\n// \tline-height: 1.8rem;\n// \tborder-radius: 2.8rem;\n// }\n\n// .hero--primary {\n// \twidth: 17.3rem;\n// \theight: 4.8rem;\n// \tbackground: $dark-pink;\n// \tcolor: $link-water-white;\n// \t&:hover,\n// \t&:focus {\n// \t\tbackground: $charm-pink;\n// \t}\n// }\n\n// .cta--positioning {\n// \tposition: absolute;\n// \ttop: 0.15rem;\n// \tright: -1rem;\n// \tborder: none;\n// \tcursor: pointer;\n\n// \t@include breakpoint(phone) {\n// \t\tposition: unset;\n// \t\twidth: 100%;\n// \t\tmargin-top: 1.6rem;\n// \t}\n// }\n","scss/base/_typography.scss":"@use '../abstracts/' as *;\n\nh1,\nh2,\nh3,\nh4 {\n\ttext-transform: uppercase;\n\tcolor: var(--white);\n}\n\nh1 {\n\tfont-size: 80px;\n\tline-height: 104px;\n\tfont-family: var(--primary-font);\n\tfont-weight: normal;\n}\n\nh2 {\n\tfont-size: 40px;\n\tline-height: 52px;\n\tletter-spacing: -1.5px;\n\tfont-family: var(--primary-font);\n\tfont-weight: normal;\n}\n\nh3 {\n\tfont-family: var(--secondary-font);\n\tfont-size: 12px;\n\tline-height: 25px;\n\tfont-weight: 700;\n\tletter-spacing: 2.57143px;\n}\n\nh4 {\n\tfont-family: var(--secondary-font);\n\tfont-size: 11px;\n\tline-height: 25px;\n\t/* identical to box height, or 227% */\n\tfont-weight: 700;\n\tletter-spacing: 1px;\n}\n","scss/layout/_navigation.scss":"@use '../abstracts' as *;\n.nav {\n\tdisplay: flex;\n\tjustify-content: space-between;\n\talign-items: center;\n\theight: 8.5rem;\n\tborder-bottom: 1px solid rgba(255, 255, 255, 0.2);\n\tpadding: 0 3.2rem;\n\t@include breakpoint(tablet) {\n\t\tflex-direction: column;\n\t\tjustify-content: center;\n\t\theight: 15.9rem;\n\t\tgap: 3.9rem;\n\t}\n\n\t&__right {\n\t\tdisplay: flex;\n\t\tgap: 3.3rem;\n\t}\n\n\t&--title {\n\t\tfont-family: var(--secondary-font);\n\t\tfont-size: 2.8rem;\n\t\tline-height: 3.6rem;\n\t\tletter-spacing: -1.05px;\n\t\ttext-transform: uppercase;\n\t}\n\n\t&--items {\n\t\tfont-family: var(--primary-font);\n\t\tfont-weight: var(--font-weight-bold);\n\t\tfont-size: 1.1rem;\n\t\tline-height: 2.5rem;\n\t\tletter-spacing: 1px;\n\t\ttext-transform: uppercase;\n\t}\n}\n","scss/abstracts/_breakpoints.scss":"$breakpoints: (\n\t'phone': 600px,\n\t'tablet': 768px,\n\t'medium': 960px,\n);\n@mixin breakpoint($point) {\n\t// $breakpoint-value: map-get($breakpoints, $size);\n\t$size: map-get($breakpoints, $point);\n\t@media only screen and (max-width: $size) {\n\t\t// includes all the content you're writing in your mixin\n\t\t@content;\n\t}\n}\n\n// @include breakpoint(tablet) {\n// }\n"},"lineCount":null}},"error":null,"hash":"27bd2771f7425a92f2a9c41fc62e9f88","cacheData":{"env":{}}}